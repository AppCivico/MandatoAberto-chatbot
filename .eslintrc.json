{
    "extends": "airbnb-base",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "no-console": 0,
        "indent": [
            "error",
            "tab"
        ],
        "no-eval": 0,
        "no-await-in-loop": 0,
        "no-tabs": 0,
        "no-param-reassign": 0,
        "camelcase": 0,
        "prefer-destructuring": 0,
        "default-case": 0,
        "max-len": [
            "error",
            195,
            {
                "code": 120,
                "ignoreUrls": true,
                "ignoreStrings": true,
                "ignoreTemplateLiterals": true
            }
        ]
    }
}